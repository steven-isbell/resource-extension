{
  "name": "Resource Collector",
  "version": "1.0",
  "description": "Add a resource to https://github.com/stevenisbell/resources.",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/favicon-16x16.png",
      "32": "assets/favicon-32x32.png",
      "144": "assets/ms-icon-144x144.png"
    },
    "default_title": "Add Resource"
  },
  "icons": {
    "16": "assets/favicon-16x16.png",
    "32": "assets/favicon-32x32.png",
    "144": "assets/ms-icon-144x144.png"
  }
}
